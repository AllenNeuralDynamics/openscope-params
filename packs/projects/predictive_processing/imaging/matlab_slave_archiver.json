{
  "$schema": "https://raw.githubusercontent.com/AllenNeuralDynamics/openscope-params/main/tooling/model_launcher.schema.json",
  "launcher_version": ">=0.2.7",
  "launcher": "matlab",
  "output_root_folder": "F:/slap2Data",
  "matlab_engine_name": "slap2_launcher",
  "matlab_entrypoint": "slap2_launcher",
  "script_parameters": {
    "rig_description_path": "C:/Users/ScanImage/Documents/slap2/configuration.json"
  },

  "session_sync_role": "slave",
  "session_sync_port": 50001,
  "session_sync_master_host": "10.128.53.175",

  "pre_acquisition_pipeline": [
    {
      "module_type": "launcher_module",
      "module_path": "experiment_notes_editor",
      "module_parameters": {
        "experiment_notes_launch_editor": true,
        "experiment_notes_filename": "{output_session_folder}\\notes\\experiment_notes.txt"
      }
    }
  ],

  "post_acquisition_pipeline": [
    {
      "module_type": "launcher_module",
      "module_path": "slap2_meta_annotator",
      "module_parameters": {
        "source_dir": "{output_session_folder}",
        "manifest_path": "{output_session_folder}\\launcher_metadata\\routing_manifest.json"
      }
    },
    {
      "module_type": "launcher_module",
      "module_path": "experiment_notes_finalize",
      "module_parameters": {
        "experiment_notes_filename": "{output_session_folder}\\notes\\experiment_notes.txt",
        "experiment_notes_confirm_prompt": "Confirm experiment notes are saved; press Enter to finish."
      }
    },
    {
      "module_type": "launcher_module",
      "module_path": "session_archiver",
      "module_parameters": {
        "network_dir": "\\\\allen\\aind\\scratch\\OpenScope\\Slap2\\Data\\{subject_id}\\{session_uuid}\\slap2",
        "backup_dir": "C:\\BonsaiDataPredictiveProcessing\\Archive\\{subject_id}\\{session_uuid}\\slap2",
        "routing_manifest": "{output_session_folder}\\launcher_metadata\\routing_manifest.json",
        "remove_empty_dirs": false,
        "skip_completed": true,
        "max_retries": 3
      }
    }
  ]
}




